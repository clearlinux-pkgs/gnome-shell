From 81ab2865f76b098affe22a0f065791e8922ad730 Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?Florian=20M=C3=BCllner?= <fmuellner@gnome.org>
Date: Mon, 19 Aug 2019 21:18:54 +0200
Subject: [PATCH 108/152] cleanup: Don't use Array/Object constructors

The corresponding literals are more concise and cleaner.

https://gitlab.gnome.org/GNOME/gnome-shell/merge_requests/731
---
 js/ui/altTab.js   | 8 ++++----
 js/ui/keyboard.js | 7 ++++---
 2 files changed, 8 insertions(+), 7 deletions(-)

diff --git a/js/ui/altTab.js b/js/ui/altTab.js
index 98ca5b386..46f4ed912 100644
--- a/js/ui/altTab.js
+++ b/js/ui/altTab.js
@@ -877,9 +877,9 @@ class ThumbnailList extends SwitcherPopup.SwitcherList {
     _init(windows) {
         super._init(false);
 
-        this._labels = new Array();
-        this._thumbnailBins = new Array();
-        this._clones = new Array();
+        this._labels = [];
+        this._thumbnailBins = [];
+        this._clones = [];
         this._windows = windows;
 
         for (let i = 0; i < windows.length; i++) {
@@ -940,7 +940,7 @@ class ThumbnailList extends SwitcherPopup.SwitcherList {
         }
 
         // Make sure we only do this once
-        this._thumbnailBins = new Array();
+        this._thumbnailBins = [];
     }
 
     _removeThumbnail(source, clone) {
diff --git a/js/ui/keyboard.js b/js/ui/keyboard.js
index 68071506f..183f82138 100644
--- a/js/ui/keyboard.js
+++ b/js/ui/keyboard.js
@@ -104,9 +104,10 @@ class KeyContainer extends St.Widget {
         this._currentRow++;
         this._currentCol = 0;
 
-        let row = new Object();
-        row.keys = [];
-        row.width = 0;
+        let row = {
+            keys: [],
+            width: 0,
+        };
         this._rows.push(row);
     }
 
